{
    "name": "sensiolabs-de/astrunner",
    "description": "Astrunner is a low level lib that helps to run static code analysis",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "nikic/php-parser": "^3.0 || ^4.0",
        "symfony/event-dispatcher": "^2.8 || ^3.0 || ^4.0"
    },
    "authors": [
        {
            "name": "timglabisch",
            "email": "astrunner+tim.glabisch@sensiolabs.de"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SensioLabs\\AstRunner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\SensioLabs\\AstRunner\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "abandoned": true
}
